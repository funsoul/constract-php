{
    "name": "funsoul/contract-php",
    "description": "A toy that implements contractual design",
    "keywords": ["contract", "annotations", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "funsoul",
            "email": "912387963@qq.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "doctrine/annotations": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Contract\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContractTest\\": "tests",
            "ContractExamples\\": "examples"
        }
    }
}
